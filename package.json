{
  "name": "uxquotes",
  "version": "1.0.0",
  "description": "Your daily dosage of UX motivation",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run compile-scss -- --style compressed",
    "dev":"npm run start-server && npm run watch-scss",
    "start-server": "serve --listen 3000",
    "compile-scss": "sass assets/scss/style.scss:assets/css/style.css",
    "watch-scss": "npm run compile-scss -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kv96/uxquotes.git"
  },
  "keywords": [
    "UX",
    "User",
    "Experience",
    "UX",
    "Motivation",
    "UX",
    "Inspiration"
  ],
  "author": "Karthik Venkataraman M",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kv96/uxquotes/issues"
  },
  "homepage": "https://github.com/kv96/uxquotes#readme",
  "dependencies": {
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "serve": "^11.3.2"
  }
}
